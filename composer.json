{
    "name": "givemelib/laravel-zip",
    "description": "laravel-zip utility ",
    "license": "MIT",
    "homepage": "http://givemelib.rf.gd",
    "authors": [
        {
            "name": "Davian Kohler",
            "email": "nomatter@blackmail.com",
            "homepage": "http://givemelib.rf.gd"
        }
    ],
    "keywords": [
        "laravel-zip",
        "laravel",
        "laravel5",
        "zip",
        "unzip",
        "extract",
        "backup",
        "ZipArchive",
        "multiple",
        "merge"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "GiveMeLib\\Zip\\ZipServiceProvider"
            ],
            "aliases": {
                "Zip": "GiveMeLib\\Zip\\ZipFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
           "GiveMeLib\\Zip\\": "src"
         }
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "scrutinizer/ocular": "^1.0"
    }
}
